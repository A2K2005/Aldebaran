{
    "extends": [
        "airbnb",
        "plugin:prettier/recommended",
        "plugin:jsx-a11y/recommended"
    ],
    "plugins": [
        "prettier",
        "jsx-a11y"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2017
    },

    "env": {
        "es6": true
    },
    "rules": {
        "guard-for-in": "off",
        "no-restricted-syntax": "off",
        "no-console": "off",
        "no-param-reassign": "warn",
        "no-extend-native": "off",
        "no-plusplus": "off",
        "import/no-webpack-loader-syntax": "off"
    }
}